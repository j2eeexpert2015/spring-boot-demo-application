{
  "dashboard": {
    "id": null,
    "uid": "load-test-003",
    "title": "ðŸš€ Load Test Analysis",
    "description": "Load testing analysis comparing Platform vs Virtual Threads under stress",
    "tags": ["load-test", "performance", "threads", "stress-test"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-10m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "ðŸŽ¯ Request Throughput Comparison",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_seconds_count{job=~\"spring-boot.*\", uri=~\"/api/.*\"}[1m])) by (thread_type)",
            "legendFormat": "{{thread_type}} RPS",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 10
                },
                {
                  "color": "red",
                  "value": 50
                }
              ]
            },
            "unit": "reqps",
            "min": 0
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        },
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "âš¡ Response Time Under Load",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\", uri=~\"/api/.*\"}[2m])) by (le, thread_type))",
            "legendFormat": "{{thread_type}} - 50th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\", uri=~\"/api/.*\"}[2m])) by (le, thread_type))",
            "legendFormat": "{{thread_type}} - 95th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\", uri=~\"/api/.*\"}[2m])) by (le, thread_type))",
            "legendFormat": "{{thread_type}} - 99th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (seconds)",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "xAxis": {
          "show": true
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "points": false,
        "pointradius": 2,
        "bars": false,
        "stack": false,
        "percentage": false,
        "legend": {
          "show": true,
          "values": false,
          "min": false,
          "max": false,
          "current": false,
          "total": false,
          "avg": false
        },
        "nullPointMode": "null",
        "steppedLine": false,
        "tooltip": {
          "value_type": "individual"
        },
        "gridPos": {
          "h": 8,
          "w": 16,
          "x": 8,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "ðŸ”¥ Thread Pool Saturation",
        "type": "graph",
        "targets": [
          {
            "expr": "tomcat_threads_busy_threads{job=~\"spring-boot.*\"} / tomcat_threads_config_max_threads{job=~\"spring-boot.*\"} * 100",
            "legendFormat": "{{thread_type}} - Thread Pool Utilization %",
            "refId": "A"
          },
          {
            "expr": "jvm_threads_live_threads{job=~\"spring-boot.*\"}",
            "legendFormat": "{{thread_type}} - Total Live Threads",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Utilization % / Thread Count",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 6
        }
      },
      {
        "id": 4,
        "title": "ðŸ“Š Concurrent Requests Handling",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_seconds_count{job=~\"spring-boot.*\", uri=~\"/api/blocking-io.*\"}[1m])) by (thread_type)",
            "legendFormat": "{{thread_type}} - Blocking I/O RPS",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_server_requests_seconds_count{job=~\"spring-boot.*\", uri=~\"/api/concurrency.*\"}[1m])) by (thread_type)",
            "legendFormat": "{{thread_type}} - Concurrency RPS",
            "refId": "B"
          },
          {
            "expr": "sum(rate(http_server_requests_seconds_count{job=~\"spring-boot.*\", uri=~\"/api/sleep.*\"}[1m])) by (thread_type)",
            "legendFormat": "{{thread_type}} - Sleep RPS",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        }
      },
      {
        "id": 5,
        "title": "ðŸ§  Memory Pressure During Load",
        "type": "graph",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{job=~\"spring-boot.*\", area=\"heap\"} / jvm_memory_max_bytes{job=~\"spring-boot.*\", area=\"heap\"} * 100",
            "legendFormat": "{{thread_type}} - Heap Usage %",
            "refId": "A"
          },
          {
            "expr": "rate(jvm_gc_pause_seconds_count{job=~\"spring-boot.*\"}[1m])",
            "legendFormat": "{{thread_type}} - GC Events/sec",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Usage % / GC Events/sec",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        }
      },
      {
        "id": 6,
        "title": "ðŸš¦ Error Rate Analysis",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_seconds_count{job=~\"spring-boot.*\", status=~\"2..\"}[1m])) by (thread_type)",
            "legendFormat": "{{thread_type}} - 2xx Success",
            "refId": "A"
          },
          {
            "expr": "sum(rate(http_server_requests_seconds_count{job=~\"spring-boot.*\", status=~\"4..\"}[1m])) by (thread_type)",
            "legendFormat": "{{thread_type}} - 4xx Client Error",
            "refId": "B"
          },
          {
            "expr": "sum(rate(http_server_requests_seconds_count{job=~\"spring-boot.*\", status=~\"5..\"}[1m])) by (thread_type)",
            "legendFormat": "{{thread_type}} - 5xx Server Error",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "lines": true,
        "fill": 5,
        "linewidth": 2,
        "stack": true,
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 20
        }
      },
      {
        "id": 7,
        "title": "ðŸŽï¸ Load Test Scenarios Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\", uri=~\".*/external-api/.*\"}[2m])) by (le, thread_type))",
            "legendFormat": "{{thread_type}} - External API (95th)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\", uri=~\".*/database/.*\"}[2m])) by (le, thread_type))",
            "legendFormat": "{{thread_type}} - Database (95th)",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\", uri=~\".*/combined.*\"}[2m])) by (le, thread_type))",
            "legendFormat": "{{thread_type}} - Combined Ops (95th)",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (seconds)",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 28
        }
      },
      {
        "id": 8,
        "title": "ðŸ“ˆ Performance Metrics Summary",
        "type": "table",
        "targets": [
          {
            "expr": "avg(rate(http_server_requests_seconds_count{job=~\"spring-boot.*\"}[5m])) by (thread_type)",
            "legendFormat": "Avg RPS",
            "refId": "A",
            "instant": true
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\"}[5m])) by (le, thread_type))",
            "legendFormat": "95th Percentile (s)",
            "refId": "B",
            "instant": true
          },
          {
            "expr": "max(jvm_threads_live_threads{job=~\"spring-boot.*\"}) by (thread_type)",
            "legendFormat": "Max Threads",
            "refId": "C",
            "instant": true
          },
          {
            "expr": "max(jvm_memory_used_bytes{job=~\"spring-boot.*\", area=\"heap\"}) by (thread_type)",
            "legendFormat": "Peak Heap (bytes)",
            "refId": "D",
            "instant": true
          },
          {
            "expr": "max(system_cpu_usage{job=~\"spring-boot.*\"}) by (thread_type)",
            "legendFormat": "Peak CPU Usage",
            "refId": "E",
            "instant": true
          },
          {
            "expr": "sum(rate(http_server_requests_seconds_count{job=~\"spring-boot.*\", status=~\"5..\"}[5m])) by (thread_type) / sum(rate(http_server_requests_seconds_count{job=~\"spring-boot.*\"}[5m])) by (thread_type) * 100",
            "legendFormat": "Error Rate %",
            "refId": "F",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true,
                "instance": true,
                "job": true
              },
              "indexByName": {
                "thread_type": 0,
                "Value #A": 1,
                "Value #B": 2,
                "Value #C": 3,
                "Value #D": 4,
                "Value #E": 5,
                "Value #F": 6
              },
              "renameByName": {
                "thread_type": "Thread Type",
                "Value #A": "Avg RPS",
                "Value #B": "95th Percentile (s)",
                "Value #C": "Max Threads",
                "Value #D": "Peak Heap (bytes)",
                "Value #E": "Peak CPU Usage",
                "Value #F": "Error Rate %"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "center",
              "displayMode": "color-background"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.5
                },
                {
                  "color": "red",
                  "value": 1.0
                }
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Thread Type"
              },
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-text"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Peak Heap (bytes)"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "bytes"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Peak CPU Usage"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "percentunit"
                }
              ]
            }
          ]
        },
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 36
        }
      },
      {
        "id": 9,
        "title": "âš–ï¸ Thread Type Comparison Ratios",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_seconds_count{job=~\"spring-boot.*\", thread_type=\"virtual\"}[5m])) / sum(rate(http_server_requests_seconds_count{job=~\"spring-boot.*\", thread_type=\"platform\"}[5m]))",
            "legendFormat": "Throughput Ratio (Virtual/Platform)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\", thread_type=\"platform\"}[5m])) by (le)) / histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\", thread_type=\"virtual\"}[5m])) by (le))",
            "legendFormat": "Latency Ratio (Platform/Virtual)",
            "refId": "B"
          },
          {
            "expr": "max(jvm_threads_live_threads{job=~\"spring-boot.*\", thread_type=\"virtual\"}) / max(jvm_threads_live_threads{job=~\"spring-boot.*\", thread_type=\"platform\"})",
            "legendFormat": "Thread Count Ratio (Virtual/Platform)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.8
                },
                {
                  "color": "green",
                  "value": 1.0
                },
                {
                  "color": "blue",
                  "value": 1.2
                }
              ]
            },
            "unit": "short",
            "decimals": 2
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        },
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 44
        }
      }
    ],
