{
  "dashboard": {
    "id": null,
    "uid": "thread-performance-001",
    "title": "üßµ Thread Performance Comparison Dashboard",
    "description": "Comprehensive comparison of Platform Threads vs Virtual Threads performance metrics",
    "tags": ["spring-boot", "threads", "performance", "java", "virtual-threads"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "üèÉ‚Äç‚ôÇÔ∏è Active Threads Comparison",
        "type": "stat",
        "targets": [
          {
            "expr": "jvm_threads_live_threads{job=~\"spring-boot.*\"}",
            "legendFormat": "{{thread_type}} - Active Threads",
            "refId": "A",
            "interval": "5s"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "list",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "orange",
                  "value": 100
                },
                {
                  "color": "red",
                  "value": 200
                }
              ]
            },
            "unit": "short",
            "min": 0
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "üìä Request Rate (RPS)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count{job=~\"spring-boot.*\"}[1m])",
            "legendFormat": "{{thread_type}} - {{uri}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "xAxis": {
          "show": true
        },
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "points": false,
        "pointradius": 2,
        "bars": false,
        "stack": false,
        "percentage": false,
        "legend": {
          "show": true,
          "values": false,
          "min": false,
          "max": false,
          "current": false,
          "total": false,
          "avg": false
        },
        "nullPointMode": "null",
        "steppedLine": false,
        "tooltip": {
          "value_type": "individual"
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "‚è±Ô∏è Response Time Percentiles",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\"}[5m])) by (le, thread_type))",
            "legendFormat": "{{thread_type}} - 95th percentile",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\"}[5m])) by (le, thread_type))",
            "legendFormat": "{{thread_type}} - 50th percentile",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\"}[5m])) by (le, thread_type))",
            "legendFormat": "{{thread_type}} - 99th percentile",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Response Time (seconds)",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "üß† Memory Usage (Heap)",
        "type": "graph",
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{job=~\"spring-boot.*\", area=\"heap\"}",
            "legendFormat": "{{thread_type}} - Heap Used",
            "refId": "A"
          },
          {
            "expr": "jvm_memory_max_bytes{job=~\"spring-boot.*\", area=\"heap\"}",
            "legendFormat": "{{thread_type}} - Heap Max",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Memory (bytes)",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "fill": 1,
        "fillGradient": 3,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "üî• CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "system_cpu_usage{job=~\"spring-boot.*\"}",
            "legendFormat": "{{thread_type}} - System CPU",
            "refId": "A"
          },
          {
            "expr": "process_cpu_usage{job=~\"spring-boot.*\"}",
            "legendFormat": "{{thread_type}} - Process CPU",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "CPU Usage",
            "show": true,
            "min": 0,
            "max": 1
          },
          {
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "üßµ Thread States Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "jvm_threads_states_threads{job=~\"spring-boot.*\"}",
            "legendFormat": "{{thread_type}} - {{state}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Thread Count",
            "show": true,
            "min": 0
          },
          {
            "show": true
          }
        ],
        "stack": true,
        "fill": 5,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "üìà Tomcat Thread Pool Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "tomcat_threads_current_threads{job=~\"spring-boot.*\"}",
            "legendFormat": "{{thread_type}} - Current Threads",
            "refId": "A"
          },
          {
            "expr": "tomcat_threads_busy_threads{job=~\"spring-boot.*\"}",
            "legendFormat": "{{thread_type}} - Busy Threads",
            "refId": "B"
          },
          {
            "expr": "tomcat_threads_config_max_threads{job=~\"spring-boot.*\"}",
            "legendFormat": "{{thread_type}} - Max Threads",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Thread Count",
            "show": true,
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 8,
        "title": "üö¶ HTTP Status Codes",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_server_requests_seconds_count{job=~\"spring-boot.*\"}[5m])",
            "legendFormat": "{{thread_type}} - {{status}} - {{uri}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "show": true,
            "min": 0
          }
        ],
        "stack": false,
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 9,
        "title": "üíæ Database Connection Pool",
        "type": "graph",
        "targets": [
          {
            "expr": "hikaricp_connections_active{job=~\"spring-boot.*\"}",
            "legendFormat": "{{thread_type}} - Active Connections",
            "refId": "A"
          },
          {
            "expr": "hikaricp_connections_idle{job=~\"spring-boot.*\"}",
            "legendFormat": "{{thread_type}} - Idle Connections",
            "refId": "B"
          },
          {
            "expr": "hikaricp_connections_max{job=~\"spring-boot.*\"}",
            "legendFormat": "{{thread_type}} - Max Connections",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Connection Count",
            "show": true,
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 40
        }
      },
      {
        "id": 10,
        "title": "üìä JVM Garbage Collection",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(jvm_gc_pause_seconds_count{job=~\"spring-boot.*\"}[5m])",
            "legendFormat": "{{thread_type}} - {{gc}} - {{action}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "GC Events/sec",
            "show": true,
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 40
        }
      },
      {
        "id": 11,
        "title": "üéØ Performance Summary Table",
        "type": "table",
        "targets": [
          {
            "expr": "jvm_threads_live_threads{job=~\"spring-boot.*\"}",
            "legendFormat": "Active Threads",
            "refId": "A",
            "instant": true
          },
          {
            "expr": "rate(http_server_requests_seconds_count{job=~\"spring-boot.*\"}[5m])",
            "legendFormat": "Request Rate",
            "refId": "B",
            "instant": true
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\"}[5m])) by (le, thread_type))",
            "legendFormat": "95th Percentile Latency",
            "refId": "C",
            "instant": true
          },
          {
            "expr": "jvm_memory_used_bytes{job=~\"spring-boot.*\", area=\"heap\"}",
            "legendFormat": "Heap Memory",
            "refId": "D",
            "instant": true
          }
        ],
        "styles": [
          {
            "alias": "Thread Type",
            "align": "auto",
            "colorMode": null,
            "colors": [
              "rgba(245, 54, 54, 0.9)",
              "rgba(237, 129, 40, 0.89)",
              "rgba(50, 172, 45, 0.97)"
            ],
            "decimals": 0,
            "pattern": "thread_type",
            "thresholds": [],
            "type": "string",
            "unit": "short"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 48
        }
      }
    ],
    "templating": {
      "list": [
        {
          "allValue": null,
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "Prometheus",
          "definition": "label_values(thread_type)",
          "hide": 0,
          "includeAll": true,
          "label": "Thread Type",
          "multi": true,
          "name": "thread_type",
          "options": [],
          "query": "label_values(thread_type)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "tagValuesQuery": "",
          "tags": [],
          "tagsQuery": "",
          "type": "query",
          "useTags": false
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
