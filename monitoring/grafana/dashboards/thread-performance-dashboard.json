{
  "id": null,
  "uid": "thread-performance-001",
  "title": "üßµ Thread Performance Comparison Dashboard",
  "description": "Comprehensive comparison of Platform Threads vs Virtual Threads performance metrics",
  "tags": ["spring-boot", "threads", "performance", "java", "virtual-threads"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "5s",
  "schemaVersion": 30,
  "version": 1,
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "üèÉ‚Äç‚ôÇÔ∏è Active Threads Comparison",
      "type": "stat",
      "targets": [
        {
          "expr": "jvm_threads_live_threads{job=~\"spring-boot.*\"}",
          "legendFormat": "{{thread_type}} - Active Threads",
          "refId": "A",
          "interval": "5s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "displayMode": "list",
            "orientation": "horizontal"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 50
              },
              {
                "color": "orange",
                "value": 100
              },
              {
                "color": "red",
                "value": 200
              }
            ]
          },
          "unit": "short",
          "min": 0
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "background"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "üìä Request Rate (RPS)",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count{job=~\"spring-boot.*\"}[1m])",
          "legendFormat": "{{thread_type}} - {{uri}}",
          "refId": "A"
        }
      ],
      "yAxes": [
        {
          "label": "Requests/sec",
          "show": true,
          "min": 0
        },
        {
          "show": true
        }
      ],
      "xAxis": {
        "show": true
      },
      "lines": true,
      "fill": 1,
      "linewidth": 2,
      "points": false,
      "pointradius": 2,
      "bars": false,
      "stack": false,
      "percentage": false,
      "legend": {
        "show": true,
        "values": false,
        "min": false,
        "max": false,
        "current": false,
        "total": false,
        "avg": false
      },
      "nullPointMode": "null",
      "steppedLine": false,
      "tooltip": {
        "value_type": "individual"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "‚è±Ô∏è Response Time Percentiles",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\"}[5m])) by (le, thread_type))",
          "legendFormat": "{{thread_type}} - 95th percentile",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_server_requests_seconds_bucket{job=~\"spring-boot.*\"}[5m])) by (le, thread_type))",
          "legendFormat": "{{thread_type}} - 50th percentile",
          "refId": "B"
        }
      ],
      "yAxes": [
        {
          "label": "Response Time (seconds)",
          "show": true,
          "min": 0
        },
        {
          "show": true
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "title": "üß† Memory Usage (Heap)",
      "type": "graph",
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{job=~\"spring-boot.*\", area=\"heap\"}",
          "legendFormat": "{{thread_type}} - Heap Used",
          "refId": "A"
        }
      ],
      "yAxes": [
        {
          "label": "Memory (bytes)",
          "show": true,
          "min": 0
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 5,
      "title": "üî• CPU Usage",
      "type": "graph",
      "targets": [
        {
          "expr": "system_cpu_usage{job=~\"spring-boot.*\"}",
          "legendFormat": "{{thread_type}} - System CPU",
          "refId": "A"
        }
      ],
      "yAxes": [
        {
          "label": "CPU Usage",
          "show": true,
          "min": 0,
          "max": 1
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      }
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "links": []
}